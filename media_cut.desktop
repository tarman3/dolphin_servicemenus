[Desktop Entry]
Type=Service
MimeType=video/*;audio/*
Actions=media_cut;media_cut_2-2
X-KDE-Submenu=Media Actions
X-KDE-Priority=TopLevel
X-KDE-RequiredNumberOfUrls=1


[Desktop Action media_cut]
Name=Cut
Exec=~/.local/share/kio/servicemenus/media_cut.sh %U

[Desktop Action media_cut_2-2]
Name=Cut 2 start and 2 finish
Exec=~/.local/share/kio/servicemenus/media_cut_2-2.sh %u

